{
  "manifest_version": 3,
  "name": "Smart pdf downloader for SmartHR",
  "description": "Simplify to download pdf from smart hr payslip list.",
  "version": "0.0.2",
  "action": {
    "default_icon": {
      "19": "icon_19.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.smarthr.jp/payslips*"],
      "js": ["js/content_script.js"]
    }
  ]
}
